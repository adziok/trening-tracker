{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [{
      "name": "@nestjs/swagger",
      "options": {
        "classValidatorShim": true,
        "dtoFileNameSuffix": [".dto.ts", "Dto.ts", "*Dto.ts"],
        "controllerFileNameSuffix": [".controller.ts", "Controller.ts"]
      }
    }
    ]
  }
}
