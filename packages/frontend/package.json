{
  "name": "@trening-tracker/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mantine/core": "^4.2.7",
    "@mantine/dates": "^4.2.7",
    "@mantine/form": "^4.2.7",
    "@mantine/hooks": "^4.2.7",
    "@mantine/modals": "^4.2.7",
    "@mantine/notifications": "^4.2.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.11",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "tabler-icons-react": "^1.48.1",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "build:css": "tailwindcss build src/tailwind.css -o src/tailwind.output.css",
    "dev": "npm run build:css && export port=3001 && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trening-tracker/shared": "^0.1.0",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  }
